    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.5.3",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/ua-parser-js": "^0.7.36",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "nuxt": "^3.5.3",
    "prettier": "^2.8.8"
  }
