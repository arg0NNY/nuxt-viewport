  "keywords": [
    "nuxt",
    "module",
    "viewport",
    "nuxt-viewport",
    "mq",
    "media",
    "query",
    "queries",
    "media query",
    "media queries",
    "plugins"
  ],
  "repository": "git@github.com:mvrlin/nuxt-viewport.git",
  "license": "MIT",
  "author": {
    "name": "Igor Talpa (mvrlin)",
    "email": "mvrlin@pm.me"
  },
  "exports": {
